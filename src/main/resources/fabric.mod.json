{
  "schemaVersion": 1,
  "id": "cozyclient",
  "version": "${version}",
  "name": "cozyClient",
  "description": "An addon for Meteor Client 1.21.5.",
  "authors": [
    "cozidev"
  ],
  "contact": {
    "sources": "https://github.com/CoziSoftware/cozyClient"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
